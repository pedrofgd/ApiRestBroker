  "Recursos": [
    {
      "nome": "cep-java",
      "parametrosViaRota": ["cep"],
      "camposResposta": ["rua", "uf", "cidade", "bairro"],
      "tentarTodosProvedoresAteSucesso": false,
      "criterios": {
        "requisicoesEstimadasHora": 5000,
