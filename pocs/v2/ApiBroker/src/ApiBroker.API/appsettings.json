            "intervaloEmSegundos": 20
          },
          "metodo": "GET",
          "formatoResposta": [
            {"nomeRecebido": "state", "nomeDesejado": "uf"},
            {"nomeRecebido": "address", "nomeDesejado": "rua"},
            {"nomeRecebido": "city", "nomeDesejado": "cidade"},
            {"nomeRecebido": "district", "nomeDesejado": "bairro"}
          ]
        }
      ]
    }
  ],
  "Logging": {
    "LogLevel": {
